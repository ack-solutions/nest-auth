{
  "name": "nest-auth-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for @ackplus/nest-auth and @ackplus/nest-auth-js packages",
  "private": true,
  "scripts": {
    "build:nest-auth": "pnpm -C packages/nest-auth build",
    "build:nest-auth-js": "pnpm -C packages/nest-auth-js build",
    "build": "pnpm build:nest-auth && pnpm build:nest-auth-js",
    "build:example": "pnpm -C apps/example-app build",
    "nest-auth": "pnpm -C packages/nest-auth",
    "nest-auth-js": "pnpm -C packages/nest-auth-js",
    "example": "pnpm -C apps/example-app",
    "publish": "node scripts/publish.js",
    "postinstall": "npm --prefix packages/nest-auth/ui install"
  },
  "keywords": [
    "nestjs",
    "crud",
    "typeorm",
    "query-builder",
    "rest-api"
  ],
  "author": "Ackplus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ackplus/nest-auth"
  },
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
