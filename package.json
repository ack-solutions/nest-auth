{
  "name": "nest-auth-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for @ackplus/nest-auth and @ackplus/nest-auth-client packages",
  "private": true,
  "scripts": {
    "build": "pnpm nest-auth build && pnpm nest-auth-client build",
    "nest-auth": "pnpm -C packages/nest-auth",
    "nest-auth-client": "pnpm -C packages/nest-auth-client",
    "nest": "pnpm -C apps/example-nest",
    "react": "pnpm -C apps/example-react",
    "publish": "node scripts/publish.js",
    "postinstall": "npm --prefix packages/nest-auth/ui install"
  },
  "keywords": [
    "nestjs",
    "crud",
    "typeorm",
    "query-builder",
    "rest-api"
  ],
  "author": "Ackplus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ackplus/nest-auth"
  },
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}